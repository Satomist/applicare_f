<p id="notice"><%= notice %></p>

<h1>Listing Therapists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @therapists.each do |therapist| %>
      <tr>
        <td> <%= therapist.email %><%= link_to 'Show', therapist %></td>

      </tr>
    <% end %>
  </tbody>
</table>

